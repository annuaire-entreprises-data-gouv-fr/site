{
  "compilerOptions": {
    "esModuleInterop": true,
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "paths": {
      "#clients/*": ["../clients/*"],
      "#components/*": ["../components/*"],
      "#components-ui/*": ["../components-ui/*"],
      "#models/*": ["../models/*"],
      "#utils/*": ["../utils/*"]
    }
  },
  "ts-node": {
    "require": ["tsconfig-paths/register"]
  }
}
