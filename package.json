{
  "name": "personnes-morales",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=16.4",
    "npm": ">=7"
  },
  "scripts": {
    "start:dev:redis": "redis-server --daemonize yes",
    "start:dev:frontend": "vite --config ./frontend/vite.config.ts",
    "start:dev:next": "NODE_ENV=development next dev",
    "dev": "run-p start:dev:*",
    "build:clean": "if [ -d 'public/assets' ]; then rm -Rf public/assets; fi",
    "build:frontend": "npm run build:clean && NODE_ENV=production vite build frontend --config ./frontend/vite.config.ts",
    "build:back": "NODE_ENV=production next build",
    "build": "npm run build:frontend && npm run build:back",
    "build:analyze": "ANALYZE=true npm run build",
    "start": "NODE_ENV=production && next start",
    "test:end2end": "cypress run",
    "test:end2end:open": "cypress open",
    "test:unit": "jest --runInBand",
    "lint": "next lint",
    "script:seo": " ts-node --project ./seo-script/seo-script-tsconfig.json ./seo-script/index.ts",
    "script:add-agent": "node ./utils/cli/add-authorized-user.js",
    "storybook:start": "start-storybook -p 6006",
    "build-storybook:build": "build-storybook"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "~5.0.0",
    "@next/bundle-analyzer": "^13.2.4",
    "@sentry/browser": "^7.45.0",
    "@sentry/nextjs": "^7.45.0",
    "@sentry/tracing": "^7.45.0",
    "@types/axios-token-interceptor": "~0.2.4",
    "@types/cookies": "~0.7.7",
    "@types/node": "^18.15.10",
    "@types/qrcode": "~1.5.0",
    "@types/raven": "~2.5.3",
    "@types/react": "^18.0.29",
    "@types/react-dom": "^18.0.11",
    "@types/ua-parser-js": "~0.7.36",
    "abort-controller": "~3.0.0",
    "autoprefixer": "^10.4.14",
    "axios": "^1.3.4",
    "axios-cache-interceptor": "^1.0.1",
    "axios-oauth-client": "~2.0.2",
    "axios-token-interceptor": "~0.2.0",
    "chart.js": "^4.2.1",
    "crawler-user-agents": "^1.0.106",
    "cssnano": "^5.1.15",
    "dotenv": "~16.0.3",
    "eslint": "^8.36.0",
    "eslint-config-next": "^13.2.4",
    "form-data": "~4.0.0",
    "iron-session": "^6.3.1",
    "js-yaml-loader": "~1.2.2",
    "matomo-tracker": "~2.2.4",
    "next": "^13.2.4",
    "next-seo": "~5.15.0",
    "openid-client": "^5.4.0",
    "postcss": "~8.4.19",
    "qrcode": "~1.5.1",
    "react": "~18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "~18.2.0",
    "react-markdown": "^8.0.6",
    "react-select": "^5.7.2",
    "redis": "^4.6.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "ua-parser-js": "^1.0.34",
    "vite": "^4.2.1"
  },
  "devDependencies": {
    "@axe-core/cli": "^4.6.0",
    "@babel/core": "^7.21.3",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-vite": "^0.4.2",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@swc/jest": "~0.2.23",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "babel-loader": "^9.1.2",
    "cypress": "^12.8.1",
    "cypress-split": "^1.3.1",
    "husky": "~8.0.2",
    "jest": "^29.5.0",
    "npm-run-all": "~4.1.5",
    "ts-jest": "^29.0.5",
    "vite-tsconfig-paths": "^4.0.7"
  },
  "optionalDependencies": {
    "@swc/core-linux-x64-gnu": "^1.3.42",
    "esbuild-linux-64": "^0.15.18",
    "tsconfig-paths": "^4.1.2"
  }
}
