{
  "name": "personnes-morales",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18",
    "npm": ">=7"
  },
  "scripts": {
    "prepare": "husky install",
    "start:dev:redis": "redis-server --daemonize yes",
    "start:dev:frontend": "vite --config ./frontend/vite.config.ts",
    "start:dev:next": "NODE_ENV=development next dev",
    "dev": "run-p start:dev:*",
    "build:clean": "if [ -d 'public/assets' ]; then rm -Rf public/assets; fi",
    "build:frontend": "npm run build:clean && NODE_ENV=production vite build frontend --config ./frontend/vite.config.ts",
    "build:back": "NODE_ENV=production next build",
    "build": "npm run build:frontend && npm run build:back",
    "start": "NODE_ENV=production && next start",
    "test:end2end:run:mock": "END2END_MOCKING=enabled run-p dev test:end2end:run",
    "test:end2end:open:mock": "END2END_MOCKING=enabled run-p dev test:end2end:open",
    "test:end2end:run": "cypress run",
    "test:end2end:open": "cypress open",
    "test:unit": "jest --runInBand --testPathIgnorePatterns=clients",
    "test:api-clients": "jest clients",
    "test:api-clients:update-snapshots": "jest clients -u",
    "lint": "next lint",
    "script:seo": " ts-node --project ./seo-script/seo-script-tsconfig.json ./seo-script/index.ts"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "~5.0.0",
    "@notionhq/client": "^2.2.7",
    "@react-spring/web": "^9.7.3",
    "@sentry/browser": "^7.57.0",
    "@sentry/nextjs": "^7.57.0",
    "@types/cookies": "~0.7.7",
    "@types/node": "^20.4.1",
    "@types/qrcode": "~1.5.1",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/ua-parser-js": "~0.7.36",
    "autoprefixer": "^10.4.14",
    "axios": "^1.4.0",
    "axios-cache-interceptor": "^1.2.0",
    "canvas": "^2.11.2",
    "chart.js": "^4.3.3",
    "crawler-user-agents": "^1.0.107",
    "cssnano": "^6.0.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.46.0",
    "eslint-config-next": "^13.4.13",
    "form-data": "~4.0.0",
    "iron-session": "^6.3.1",
    "js-yaml-loader": "~1.2.2",
    "matomo-tracker": "~2.2.4",
    "next": "^13.4.9",
    "next-seo": "^6.1.0",
    "openid-client": "^5.4.3",
    "postcss": "^8.4.27",
    "qrcode": "^1.5.3",
    "react": "~18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "~18.2.0",
    "react-markdown": "^8.0.7",
    "react-select": "^5.7.4",
    "redis": "^4.6.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "ua-parser-js": "^1.0.35",
    "vite": "^4.4.9"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@swc/jest": "^0.2.26",
    "@types/jest": "^29.5.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "cypress": "^13.3.0",
    "jest-file-snapshot": "^0.5.0",
    "husky": "~8.0.3",
    "jest": "^29.6.2",
    "npm-run-all": "~4.1.5",
    "ts-jest": "^29.1.1",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "optionalDependencies": {
    "@swc/core-linux-x64-gnu": "^1.3.68",
    "esbuild-linux-64": "^0.15.18",
    "tsconfig-paths": "^4.2.0"
  }
}
