{
  "name": "personnes-morales",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18",
    "npm": ">=7"
  },
  "scripts": {
    "start:dev:redis": "redis-server --daemonize yes",
    "start:dev:frontend": "vite --config ./frontend/vite.config.ts",
    "start:dev:next": "NODE_ENV=development next dev",
    "dev": "run-p start:dev:*",
    "build:clean": "if [ -d 'public/assets' ]; then rm -Rf public/assets; fi",
    "build:frontend": "npm run build:clean && NODE_ENV=production vite build frontend --config ./frontend/vite.config.ts",
    "build:back": "NODE_ENV=production next build",
    "build": "npm run build:frontend && npm run build:back",
    "build:analyze": "ANALYZE=true npm run build",
    "start": "NODE_ENV=production && next start",
    "test:end2end": "END2END_MOCKING=enabled run-p dev test:end2end:run",
    "test:end2end:run": "cypress run",
    "test:end2end:open": "cypress open",
    "test:recipe-staging": "cypress open --config baseUrl=https://staging.annuaire-entreprises.data.gouv.fr video=false",
    "test:unit": "jest --runInBand",
    "test:a11y": "npx pa11y-ci --config ./pa11y.config.json",
    "lint": "next lint",
    "script:seo": " ts-node --project ./seo-script/seo-script-tsconfig.json ./seo-script/index.ts",
    "storybook:start": "start-storybook -p 6006",
    "build-storybook:build": "build-storybook"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "~5.0.0",
    "@next/bundle-analyzer": "^13.4.9",
    "@notionhq/client": "^2.2.7",
    "@sentry/browser": "^7.57.0",
    "@sentry/nextjs": "^7.57.0",
    "@types/cookies": "~0.7.7",
    "@types/node": "^20.4.1",
    "@types/qrcode": "~1.5.1",
    "@types/raven": "~2.5.4",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/ua-parser-js": "~0.7.36",
    "abort-controller": "~3.0.0",
    "autoprefixer": "^10.4.14",
    "axios": "^1.4.0",
    "axios-cache-interceptor": "^1.2.0",
    "canvas": "^2.11.2",
    "chart.js": "^4.3.0",
    "crawler-user-agents": "^1.0.107",
    "cssnano": "^6.0.1",
    "dotenv": "~16.3.1",
    "eslint": "^8.44.0",
    "eslint-config-next": "^13.4.9",
    "form-data": "~4.0.0",
    "iron-session": "^6.3.1",
    "js-yaml-loader": "~1.2.2",
    "matomo-tracker": "~2.2.4",
    "next": "^13.4.9",
    "next-seo": "^6.1.0",
    "openid-client": "^5.4.3",
    "postcss": "^8.4.25",
    "qrcode": "^1.5.3",
    "react": "~18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "~18.2.0",
    "react-markdown": "^8.0.7",
    "react-select": "^5.7.3",
    "redis": "^4.6.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "ua-parser-js": "^1.0.35",
    "vite": "^4.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.22.8",
    "@storybook/addon-actions": "^7.0.26",
    "@storybook/addon-essentials": "^7.0.26",
    "@storybook/addon-interactions": "^7.0.26",
    "@storybook/addon-links": "^7.0.26",
    "@storybook/builder-vite": "^7.0.26",
    "@storybook/react": "^7.0.26",
    "@storybook/testing-library": "^0.2.0",
    "@swc/jest": "^0.2.26",
    "@types/jest": "^29.5.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "babel-loader": "^9.1.3",
    "cypress": "^12.17.1",
    "husky": "~8.0.3",
    "jest": "^29.6.1",
    "npm-run-all": "~4.1.5",
    "ts-jest": "^29.1.1",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "optionalDependencies": {
    "@swc/core-linux-x64-gnu": "^1.3.68",
    "esbuild-linux-64": "^0.15.18",
    "tsconfig-paths": "^4.2.0"
  }
}
