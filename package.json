{
  "name": "annuaire-entreprises-site",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20",
    "npm": ">=10"
  },
  "scripts": {
    "prepare": "npx simple-git-hooks; npm run script:fetch-protected-siren",
    "start:dev:redis": "redis-server --daemonize yes",
    "start:dev:next": "NODE_ENV=development next dev",
    "dev": "run-p start:dev:*",
    "build": "NODE_ENV=production next build || NODE_ENV=production SENTRY_DISABLE_SOURCEMAP_UPLOAD=true next build",
    "start": "NODE_ENV=production && next start",
    "test:end2end:run:mock": "NEXT_PUBLIC_END2END_MOCKING=enabled run-p dev test:end2end:run",
    "test:end2end:open:mock": "NEXT_PUBLIC_END2END_MOCKING=enabled run-p dev test:end2end:open",
    "test:end2end:run": "cypress run",
    "test:end2end:open": "cypress open",
    "test:unit": "jest --runInBand --testPathIgnorePatterns=clients",
    "test:api-clients": "jest clients --silent",
    "test:api-clients:update-snapshots": "jest clients --silent -u",
    "lint": "next lint",
    "script:fetch-protected-siren": "ts-node ./scripts/fetch-protected-siren.ts"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "~5.0.0",
    "@react-spring/web": "^9.7.3",
    "@sentry/browser": "^7.94.1",
    "@sentry/nextjs": "^7.94.1",
    "@types/cookies": "^0.7.10",
    "@types/node": "^20.11.5",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@vcarl/remark-headings": "^0.1.0",
    "autoprefixer": "^10.4.17",
    "axios": "^1.6.5",
    "axios-cache-interceptor": "^1.5.1",
    "body-parser": "^1.20.2",
    "canvas": "^2.11.2",
    "chart.js": "^4.4.1",
    "crawler-user-agents": "^1.0.117",
    "cssnano": "^6.0.3",
    "dotenv": "^16.3.2",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "form-data": "~4.0.0",
    "iron-session": "^8.0.1",
    "js-yaml-loader": "~1.2.2",
    "matomo-tracker": "~2.2.4",
    "next": "^14.1.0",
    "next-seo": "^6.4.0",
    "openid-client": "^5.6.4",
    "postcss": "^8.4.33",
    "qrcode": "^1.5.3",
    "react": "~18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "~18.2.0",
    "react-select": "^5.8.0",
    "redis": "^4.6.12",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-stringify": "^10.0.0",
    "remark-heading-id": "^1.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.0",
    "typescript": "^5.3.3",
    "unified": "^11.0.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.5.0",
    "@commitlint/config-conventional": "^18.5.0",
    "@swc/jest": "^0.2.31",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "cypress": "^13.6.3",
    "jest": "^29.7.0",
    "jest-file-snapshot": "^0.5.0",
    "npm-run-all": "~4.1.5",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^9.4.0",
    "simple-git-hooks": "^2.9.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2"
  },
  "optionalDependencies": {
    "@swc/core-linux-x64-gnu": "^1.3.105",
    "esbuild-linux-64": "^0.15.18",
    "tsconfig-paths": "^4.2.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npm run lint && npm run test:unit"
  }
}
