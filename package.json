{
  "name": "personnes-morales",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18",
    "npm": ">=7"
  },
  "scripts": {
    "prepare": "npx simple-git-hooks; npm run script:fetch-protected-siren",
    "start:dev:redis": "redis-server --daemonize yes",
    "start:dev:frontend": "vite --config ./frontend/vite.config.ts",
    "start:dev:next": "NODE_ENV=development next dev",
    "dev": "run-p start:dev:*",
    "build:clean": "if [ -d 'public/assets' ]; then rm -Rf public/assets; fi",
    "build:frontend": "npm run build:clean && NODE_ENV=production vite build frontend --config ./frontend/vite.config.ts",
    "build:back": "NODE_ENV=production next build || NODE_ENV=production SENTRY_DISABLE_SOURCEMAP_UPLOAD=true next build",
    "build": "npm run build:frontend && npm run build:back",
    "start": "NODE_ENV=production && next start",
    "test:end2end:run:mock": "END2END_MOCKING=enabled run-p dev test:end2end:run",
    "test:end2end:open:mock": "END2END_MOCKING=enabled run-p dev test:end2end:open",
    "test:end2end:run": "cypress run",
    "test:end2end:open": "cypress open",
    "test:unit": "jest --runInBand --testPathIgnorePatterns=clients",
    "test:api-clients": "jest clients --silent",
    "test:api-clients:update-snapshots": "jest clients --silent -u",
    "lint": "next lint",
    "script:fetch-protected-siren": "ts-node ./scripts/fetch-protected-siren.ts",
    "script:seo": " ts-node ./scripts/seo/index.ts"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "~5.0.0",
    "@react-spring/web": "^9.7.3",
    "@sentry/browser": "^7.77.0",
    "@sentry/nextjs": "^7.77.0",
    "@types/cookies": "^0.7.8",
    "@types/node": "^20.8.6",
    "@types/qrcode": "^1.5.2",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@vcarl/remark-headings": "^0.1.0",
    "autoprefixer": "^10.4.16",
    "axios": "^1.5.1",
    "axios-cache-interceptor": "^1.3.1",
    "canvas": "^2.11.2",
    "chart.js": "^4.4.0",
    "crawler-user-agents": "^1.0.114",
    "cssnano": "^6.0.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.51.0",
    "eslint-config-next": "^13.5.5",
    "form-data": "~4.0.0",
    "iron-session": "^6.3.1",
    "js-yaml-loader": "~1.2.2",
    "matomo-tracker": "~2.2.4",
    "next": "^13.5.5",
    "next-seo": "^6.1.0",
    "openid-client": "^5.6.1",
    "postcss": "^8.4.31",
    "qrcode": "^1.5.3",
    "react": "~18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "~18.2.0",
    "react-select": "^5.7.7",
    "redis": "^4.6.10",
    "rehype-autolink-headings": "^7.0.0",
    "rehype-stringify": "^10.0.0",
    "remark-heading-id": "^1.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "unified": "^11.0.3",
    "vite": "^4.4.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.8.0",
    "@commitlint/config-conventional": "^17.8.0",
    "@swc/jest": "^0.2.29",
    "@types/jest": "^29.5.5",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "cypress": "^13.3.1",
    "jest": "^29.7.0",
    "jest-file-snapshot": "^0.5.0",
    "npm-run-all": "~4.1.5",
    "simple-git-hooks": "^2.9.0",
    "ts-jest": "^29.1.1",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "optionalDependencies": {
    "@swc/core-linux-x64-gnu": "^1.3.68",
    "esbuild-linux-64": "^0.15.18",
    "tsconfig-paths": "^4.2.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npm run lint && npm run test:unit"
  }
}
