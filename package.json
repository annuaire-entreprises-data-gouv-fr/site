{
  "name": "personnes-morales",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=16.4",
    "npm": ">=7"
  },
  "scripts": {
    "start:dev:redis": "redis-server --daemonize yes",
    "start:dev:frontend": "vite --config ./frontend/vite.config.ts",
    "start:dev:next": "NODE_ENV=development next dev",
    "dev": "run-p start:dev:*",
    "build:clean": "if [ -d 'public/assets' ]; then rm -Rf public/assets; fi",
    "build:frontend": "npm run build:clean && NODE_ENV=production vite build frontend --config ./frontend/vite.config.ts",
    "build:back": "NODE_ENV=production next build",
    "build": "npm run build:frontend && npm run build:back",
    "start": "NODE_ENV=production && next start",
    "test:end2end": "cypress open",
    "test:unit": "jest --runInBand",
    "lint": "next lint",
    "sitemap": "node ./node-scripts/generate-sitemap.js"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@sentry/browser": "^7.11.1",
    "@sentry/nextjs": "^7.13.0",
    "@sentry/tracing": "^7.13.0",
    "@types/axios-token-interceptor": "^0.2.3",
    "@types/cookies": "^0.7.7",
    "@types/node": "^18.7.14",
    "@types/qrcode": "^1.5.0",
    "@types/raven": "^2.5.3",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "@types/ua-parser-js": "^0.7.36",
    "@vitejs/plugin-legacy": "^2.0.1",
    "abort-controller": "^3.0.0",
    "autoprefixer": "^10.4.8",
    "axios": "^0.27.2",
    "axios-cache-interceptor": "^0.10.7",
    "axios-oauth-client": "^1.5.0",
    "axios-token-interceptor": "^0.2.0",
    "crawler-user-agents": "^1.0.92",
    "cssnano": "^5.1.13",
    "dotenv": "^16.0.2",
    "eslint": "^8.23.0",
    "eslint-config-next": "^12.2.5",
    "form-data": "^4.0.0",
    "js-yaml-loader": "^1.2.2",
    "matomo-tracker": "^2.2.4",
    "next": "^12.2.5",
    "next-connect": "^0.13.0",
    "next-seo": "^5.5.0",
    "postcss": "^8.4.16",
    "qrcode": "^1.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.3",
    "redis": "^4.3.0",
    "typescript": "^4.8.2",
    "ua-parser-js": "^1.0.2",
    "vite": "^3.0.9"
  },
  "devDependencies": {
    "@axe-core/cli": "^4.4.4",
    "@swc/jest": "^0.2.22",
    "@types/jest": "^29.0.0",
    "cypress": "^10.7.0",
    "jest": "^29.0.1",
    "npm-run-all": "^4.1.5",
    "pre-commit": "^1.2.2"
  },
  "optionalDependencies": {
    "@swc/core-linux-x64-gnu": "^1.2"
  },
  "pre-commit": {
    "run": [
      "lint",
      "test:unit"
    ]
  }
}
