{
  "name": "personnes-morales",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=16.4",
    "npm": ">=7"
  },
  "scripts": {
    "start:dev:redis": "redis-server --daemonize yes",
    "start:dev:frontend": "vite --config ./frontend/vite.config.ts",
    "start:dev:next": "NODE_ENV=development next dev",
    "dev": "run-p start:dev:*",
    "build:clean": "if [ -d 'public/assets' ]; then rm -Rf public/assets; fi",
    "build:frontend": "npm run build:clean && NODE_ENV=production vite build frontend --config ./frontend/vite.config.ts",
    "build:back": "NODE_ENV=production next build",
    "build": "npm run build:frontend && npm run build:back",
    "start": "NODE_ENV=production && next start",
    "test:end2end": "cypress run",
    "test:end2end:open": "cypress open",
    "test:unit": "jest --runInBand",
    "lint": "next lint",
    "script:seo": " ts-node --project ./seo-script/seo-script-tsconfig.json ./seo-script/index.ts",
    "storybook:start": "start-storybook -p 6006",
    "build-storybook:build": "build-storybook"
  },
  "dependencies": {
    "@fullhuman/postcss-purgecss": "~5.0.0",
    "@next/bundle-analyzer": "^13.1.5",
    "@sentry/browser": "~7.24.2",
    "@sentry/nextjs": "~7.24.2",
    "@sentry/tracing": "~7.24.2",
    "@types/axios-token-interceptor": "~0.2.4",
    "@types/cookies": "~0.7.7",
    "@types/node": "~18.11.11",
    "@types/qrcode": "~1.5.0",
    "@types/raven": "~2.5.3",
    "@types/react": "~18.0.26",
    "@types/react-dom": "~18.0.9",
    "@types/ua-parser-js": "~0.7.36",
    "abort-controller": "~3.0.0",
    "autoprefixer": "~10.4.13",
    "axios": "~1.2.1",
    "axios-cache-interceptor": "~0.10.7",
    "axios-oauth-client": "~2.0.2",
    "axios-token-interceptor": "~0.2.0",
    "crawler-user-agents": "~1.0.100",
    "cssnano": "~5.1.14",
    "dotenv": "~16.0.3",
    "eslint": "~8.29.0",
    "eslint-config-next": "~13.0.6",
    "form-data": "~4.0.0",
    "js-yaml-loader": "~1.2.2",
    "matomo-tracker": "~2.2.4",
    "next": "~13.0.6",
    "next-connect": "~0.13.0",
    "next-seo": "~5.15.0",
    "postcss": "~8.4.19",
    "qrcode": "~1.5.1",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "react-markdown": "~8.0.4",
    "redis": "~4.5.1",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.4",
    "ua-parser-js": "~1.0.32",
    "vite": "~3.2.5"
  },
  "devDependencies": {
    "@axe-core/cli": "~4.5.2",
    "@babel/core": "^7.20.7",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-vite": "^0.2.6",
    "@storybook/react": "^6.5.15",
    "@storybook/testing-library": "^0.0.13",
    "@swc/jest": "~0.2.23",
    "@types/jest": "~29.2.4",
    "@typescript-eslint/eslint-plugin": "~5.46.1",
    "babel-loader": "^8.3.0",
    "cypress": "~12.0.1",
    "husky": "~8.0.2",
    "jest": "~29.3.1",
    "npm-run-all": "~4.1.5",
    "ts-jest": "~29.0.3",
    "vite-tsconfig-paths": "^4.0.3"
  },
  "optionalDependencies": {
    "@swc/core-linux-x64-gnu": "~1.3.21",
    "tsconfig-paths": "^4.1.2"
  }
