{
  "compilerOptions": {
    "baseUrl": ".",
    "allowJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "paths": {
      "#clients/*": [
        "./clients/*"
      ],
      "#cypress/*": [
        "./cypress/*"
      ],
      "#components/*": [
        "./components/*"
      ],
      "#components-ui/*": [
        "./components-ui/*"
      ],
      "#models/*": [
        "./models/*"
      ],
      "#utils/*": [
        "./utils/*"
      ],
      "#hooks/*": [
        "./hooks/*"
      ]
    },
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "es6",
    "types": [
      "jest",
      "webpack-env"
    ],
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strictNullChecks": true
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ]
}